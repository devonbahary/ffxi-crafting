{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard-with-typescript",
        "plugin:react/recommended",
        "prettier"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier", "react"],
    "rules": {
        "prettier/prettier": ["error"]
    },
    "settings": {
        "react": {
            "version": "18.2"
        }
    }
}
